{
    "compilerOptions": {
        "jsx": "react-jsx",
        "strict": true,
        "removeComments": true,
        "sourceMap": false,
        // "noImplicitAny": true,
        // "noUnusedLocals": true,
        // "noUnusedParameters": true,
        "strictNullChecks": false,
        "skipLibCheck": true,
        "noEmit": true,
        "esModuleInterop": true
    },
    "include": ["src/*", "src/**/*", "src/**/**/*"],
    "exclude": ["node_modules", "src/**/*.test.tsx"]
}
